<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.project"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.root.project.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.root.project.Reg"
            android:label="@string/title_activity_reg"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.root.project.Reg_final"
            android:label="@string/title_activity_reg_final"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.root.project.ImageButton"
            android:label="@string/title_activity_image_button"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.root.project.Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />

        <service android:name="com.example.root.project.NewIntent" />
        <service
            android:name="com.example.root.project.MyService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>